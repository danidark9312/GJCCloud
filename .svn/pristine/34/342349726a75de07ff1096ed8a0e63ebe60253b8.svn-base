package com.sf.roles;

import java.util.List;

/**
 * Sirve como contenedor que contiene los permisos (acciones/opciones) 
 * sobre la vista y los roles que pueden accesar
 * @author USUARIO
 */
public class MenuVistaPermisosRolesWrapper {
	List<RolAccionOpcion> listaRolAccionOpcion;
	List<Opcion> opciones;
	Rol rol;
	List<Rol> roles;
	/**
	 * @return the listaRolAccionOpcion
	 */
	public List<RolAccionOpcion> getListaRolAccionOpcion() {
		return listaRolAccionOpcion;
	}
	/**
	 * @param listaRolAccionOpcion the listaRolAccionOpcion to set
	 */
	public void setListaRolAccionOpcion(List<RolAccionOpcion> listaRolAccionOpcion) {
		this.listaRolAccionOpcion = listaRolAccionOpcion;
	}
	/**
	 * @return the opciones
	 */
	public List<Opcion> getOpciones() {
		return opciones;
	}
	/**
	 * @param opciones the opciones to set
	 */
	public void setOpciones(List<Opcion> opciones) {
		this.opciones = opciones;
	}
	/**
	 * @return the rol
	 */
	public Rol getRol() {
		return rol;
	}
	/**
	 * @param rol the rol to set
	 */
	public void setRol(Rol rol) {
		this.rol = rol;
	}
	/**
	 * @return the roles
	 */
	public List<Rol> getRoles() {
		return roles;
	}
	/**
	 * @param roles the roles to set
	 */
	public void setRoles(List<Rol> roles) {
		this.roles = roles;
	}
	
	
	
}
